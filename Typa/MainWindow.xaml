<Window x:Class="Typa.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Typa"
        mc:Ignorable="d"
        xmlns:views="clr-namespace:Typa.Views"
        xmlns:tb="http://www.hardcodet.net/taskbar" 
         Height="800" Width="1000">
    <Grid>
        <tb:TaskbarIcon x:Name="notifyIcon" IconSource="/Icons/Inactive.ico" ToolTipText="Typa" />

        <views:TypaView  x:Name="view" />
    </Grid>
</Window>
